---
import GithubCalendar from "./ui/GithubCalendar"
import GithubRepos from '@/components/ui/GithubRepos';

---

<section id="github-integration" class=" w-fit mx-auto">
        <div class="mb-10">
            <h1 class="text-2xl font-bold mb-2"> There's More On The Way</h1>
            <p> Check back in to see some interesting new projects and updates</p>
        </div>
        <div class="flex flex-col gap-y-12">
            <GithubCalendar client:only="react"/>
            <GithubRepos client:load/>
        </div>
</section>